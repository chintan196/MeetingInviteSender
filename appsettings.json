{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ApplicationDisplayName": "Send Calendar Invite Email",
  "ApplicationSettings": {
    "PreferredSmtp": "Microsoft",
    // Please note that for your meeting invitations to be displayed properly by clients (Gmail, Outlook) 
    // its compulsory to use the same email (email address specified as User Name below) as an Organiser for the meeting.
    // Otherwise some client will not parse ICS correctly.
    "AvailableProviders": [
      {
        "SmtpType": "Microsoft",
        "Host": "smtp.office365.com",
        "Port": "587",
        "UserName": "emailAddressOfOutlook",
        "Password": "passwordOfOutlook"
      },
      {
        "SmtpType": "Google",
        "Host": "smtp.gmail.com",
        "Port": "587",
        "UserName": "emailAddressOfGmail",
        "Password": "passwordOfGmail"
      }
    ]
  }
}
